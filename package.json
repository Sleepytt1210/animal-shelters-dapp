{
  "name": "animal-shelter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/register": "^7.17.7",
    "@depay/web3-wallets": "^10.0.3",
    "@openzeppelin/contracts": "^4.5.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.1.1",
    "@truffle/contract": "^4.5.7",
    "@web3auth/web3auth": "^0.8.2",
    "antd": "^4.20.1",
    "assert": "^2.0.0",
    "chai": "^4.3.6",
    "chart.js": "^3.7.1",
    "chartjs-node-canvas": "^4.1.6",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "mocha": "^9.2.2",
    "moralis": "^1.5.8",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-blockies": "^1.4.1",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^18.1.0",
    "react-moralis": "^1.3.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "web-vitals": "^2.1.4",
    "web3": "^1.7.1"
  },
  "scripts": {
    "start": "set \"GENERATE_SOURCEMAP=false\" BROWSER='chrome' BROWSER_ARGS='--remote-debugging-port=9222' && react-app-rewired start ",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "devchain": "node devChain.js",
    "testchain": "node devChain.js --test",
    "jestchain": "node devChain.js --jest",
    "gasreport": "node ./gas-output/gas-report.js --output",
    "plotgas": "node ./gas-output/plot-charts.js",
    "truffle:test": "truffle test --network test",
    "truffle:test-log": "npm run truffle:test -- --reporter mochawesome",
    "truffle:test-gas": "set \"CI=true\" && npm run truffle:test -- --grep \"Testing normal\" --reporter eth-gas-reporter",
    "truffle:debug": "truffle debug",
    "truffle:migrate": "truffle migrate"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "resetMocks": false,
    "moduleNameMapper": {
      "^lodash-es$": "lodash",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@antv|antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx|ts|tsx)$"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./jest-report",
          "filename": "jest-report.html",
          "openReport": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@depay/web3-blockchains": "^4.3.0",
    "@depay/web3-mock": "^11.7.0",
    "@testing-library/user-event": "^14.1.1",
    "babel-jest": "^27.5.1",
    "dotenv": "^16.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eth-gas-reporter": "^0.2.25",
    "ganache": "^7.1.0",
    "ipfs": "^0.62.2",
    "jest-html-reporters": "^3.0.6",
    "mochawesome": "^7.1.3",
    "react-app-rewired": "^2.2.1",
    "truffle-assertions": "^0.9.2",
    "truffle-contract-size": "^2.0.1",
    "ts-jest": "^27.1.4"
  }
}
